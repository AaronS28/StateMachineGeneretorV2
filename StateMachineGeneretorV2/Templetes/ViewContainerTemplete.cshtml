using @(Model.SolutionName).Views;

namespace @(Model.SolutionName).Library.Navigation
{
    public class ViewContainer : : BaseSingleton<ViewContainer>
    {
        @Model.Properties

        private static ViewContainer instance;

        public static ViewContainer Instance
        {
            get
            {
                if (instance == null)
                    instance = new ViewContainer();
                return instance;
            }
        }

        private ViewContainer()
        {
            @Model.Constructors
        }
    }
}